<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.cw.dormitoryais.Controllers.MultifunctionalTableWindowController">
   <children>
      <TableView fx:id="multifunctionalTable" onMouseClicked="#multifunctionalTable_CellDoubleClick" prefHeight="384.0" prefWidth="884.0" VBox.vgrow="ALWAYS">
         <placeholder>
            <Label text="" />
         </placeholder>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox fx:id="buttonsBox" alignment="CENTER_RIGHT" spacing="10.0" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="detachB" mnemonicParsing="false" onAction="#detachB_Click" prefWidth="75.0" text="Открепить" />
            <Button fx:id="multifunctionalB" mnemonicParsing="false" onAction="#multifunctionalB_Click" prefWidth="75.0" text="Изменить" />
            <Button fx:id="cancelB" mnemonicParsing="false" onAction="#cancelB_Click" prefWidth="75.0" text="Отмена" />
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
